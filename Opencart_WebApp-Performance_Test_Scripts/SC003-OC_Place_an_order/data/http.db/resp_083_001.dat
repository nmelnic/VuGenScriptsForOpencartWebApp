							$('#payment-address .checkout-heading').append('<a>Modify &raquo;</a>');	
						},
						error: function(xhr, ajaxOptions, thrownError) {
							alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
						}
					});			
				}
				
				$.ajax({
					url: 'index.php?route=checkout/payment_address',
